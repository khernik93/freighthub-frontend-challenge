{
  "name": "freighthub",
  "version": "1.0.0",
  "description": "Freight Hub",
  "author": "Karol Hernik <khernik93@gmail.com>",
  "scripts": {
    "build": "npm run clean:dist && npm run sass",
    "build:prod": "npm run clean:dist && npm run sass && webpack --prod --config config/webpack.config.ts",
    "start": "npm run build && npm run webdev:server",
    "start:prod": "npm run build:prod && npm run server",
    "webdev": "webpack-dev-server --config config/webpack.config.ts",
    "webdev:hmr": "npm run webdev --hotOnly --hot --inline",
    "webdev:server": "npm-run-all -p -r webdev:hmr sass:watch",
    "sass": "node-sass \"src/styles.scss\" \"dist_root/styles.css\" --include-path node_modules --output-style compressed -q",
    "sass:watch": "node-sass -w \"src/styles.scss\" \"dist_root/styles.css\" --include-path node_modules --output-style compressed -q",
    "clean:dist": "rimraf -- dist",
    "test": "jest --coverage",
    "lint": "tslint -p tslint.json src/**/*.tsx",
    "server": "json-server -p 4000 db.json"
  },
  "dependencies": {
    "@babel/core": "7.1.2",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "@types/copy-webpack-plugin": "^4.4.3",
    "@types/mime": "~2.0.0",
    "@types/node": "~8.5.2",
    "@types/react": "^16.8.10",
    "@types/webpack": "^4.4.26",
    "awesome-typescript-loader": "^5.2.1",
    "axios": "^0.18.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "8.0.4",
    "compression-webpack-plugin": "^1.1.11",
    "connected-react-router": "^6.3.2",
    "copy-webpack-plugin": "~4.5.1",
    "css-loader": "~0.28.11",
    "datatables.net": "^1.10.19",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^4.0.0-alpha",
    "jquery": "^3.4.1",
    "json-server": "^0.14.2",
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "npm-run-all": "~4.1.3",
    "raw-loader": "~0.5.1",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-modal": "^3.8.1",
    "react-redux": "^6.0.1",
    "react-router": "^5.0.0",
