    "sass-loader": "^7.0.1",
    "source-map-loader": "~0.2.3",
    "string-replace-loader": "~2.1.1",
    "style-loader": "^0.23.1",
    "to-string-loader": "~1.1.5",
    "ts-loader": "^5.3.3",
    "ts-node": "^6.0.5",
    "typescript": "~3.2.4",
    "uglifyjs-webpack-plugin": "^1.2.5",
